class Kitten{
	public $size = "small";
	public $age = "newborn";
	public $looks = "adorable";
	public $color = "white";
}

class Friend{
	public $name = "Douglas";
	public $race = "Guateamalan";
	public $traits = "funny";
	public $gender = "male";
}

class Bunny{
	public $speed = "fast";
	public $color = "brown";
	public $fur = "soft";
	public $habitat "forest";
}




$kitten1 = new Kitten();
print $kitten1->name;

