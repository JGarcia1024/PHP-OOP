class Kitten{
	public $firstName = "Arnold";
	public $lastName = "Shwarzenegger";
	public $gender = "male";
	public $size = "size";

	function getName() {
		return "{$this->firstName}";
		"{$this->lastName}";
	}
}

$kitten1 = new Kitten();
$kitten1->firstName = "Arnold";
$kitten1->lastName = "Shwarzenegger";

print "The dog's name is{$kitten1->getName()}";

class Kitten{
	public $firstName = "Joshua";
	public $lastName = "Garcia";
	public $gender = "male";
	public $size = "size";

	function getName() {
		return "{$this->firstName}";
		"{$this->lastName}";
	}
}

$kitten2 = new Kitten();
$kitten2->firstName = "Joshua";
$kitten2->lastName = "Garcia";

print "The dog's name is{$kitten2->getName()}";

class Kitten{
	public $firstName = "Cara";
	public $lastName = "Delevingne";
	public $gender = "female";
	public $size = "size";

	function getName() {
		return "{$this->firstName}";
		"{$this->lastName}";
	}
}

$kitten3 = new Kitten();
$kitten3->firstName = "Arnold";
$kitten3->lastName = "Shwarzenegger";

print "The dog's name is{$kitten3->getName()}";